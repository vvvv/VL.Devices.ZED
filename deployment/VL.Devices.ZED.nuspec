<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>VL.Devices.ZED</id>
    <version>1.4.0</version>
    <title>VL.Devices.ZED</title>
    <authors>Alexander Shaliapin, vvvv, m-box</authors>
    <owners>vvvv</owners>
